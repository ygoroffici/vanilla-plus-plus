{
  "CONFIG_VERSION_DONT_TOUCH_THIS": 3,
  "_comment": "Before changing anything, see https://github.com/Patbox/StyledChat#configuration",
  "text_formatting": {
    "legacy_formatting": false,
    "parse_links": true,
    "markdown": false,
    "formatting_from_other_mods": false
  },
  "chat_preview": {
    "send_full_message": false,
    "require_for_formatting": false
  },
  "auto_completion": {
    "tags": true,
    "tag_aliases": true,
    "emoticons": true
  },
  "default": {
    "display_name": "${default}",
    "message_formats": {
      "chat": "<yellow>${player}</yellow><dark_gray> » <r>${message}",
      "joined_the_game": "<yellow><lang:multiplayer.player.joined:'${player}'></yellow>",
      "joined_after_name_change": "<yellow><lang:multiplayer.player.joined.renamed:'${player}':'${old_name}'></yellow>",
      "joined_for_first_time": "<yellow><lang:multiplayer.player.joined:'${player}'></yellow>",
      "left_game": "<yellow><lang:multiplayer.player.left:'${player}'></yellow>",
      "base_death": "${default_message}",
      "advancement_task": "<lang:chat.type.advancement.task:'<yellow>${player}':'${advancement}'>",
      "advancement_challenge": "<lang:chat.type.advancement.challenge:'${player}':'${advancement}'>",
      "advancement_goal": "<lang:chat.type.advancement.goal:'${player}':'${advancement}'>",
      "sent_team_chat": "<lang:'chat.type.team.sent':'<hover\\:\\'<lang\\:chat.type.team.hover>\\'><suggest_command\\:\\'/teammsg \\'>${team}':'${displayName}':'${message}'>",
      "received_team_chat": "<lang:'chat.type.team.text':'<hover\\:\\'<lang\\:chat.type.team.hover>\\'><suggest_command\\:\\'/teammsg \\'>${team}':'${displayName}':'${message}'>",
      "sent_private_message": "<dark_green>@<r> <gray><italic><lang:commands.message.display.outgoing:'<bold>${receiver}':'<dark_aqua>${message}'>",
      "received_private_message": "<dark_green>@<r> <gray><italic><lang:commands.message.display.incoming:'<bold>${sender}':'<dark_aqua>${message}'>",
      "say_command": "[${player}] ${message}",
      "me_command": "<lang:'chat.type.emote':'${player}':'${message}'>",
      "pet_death": "${default_message}"
    },
    "link_style": "<underline><c:#7878ff>${link}",
    "spoiler_style": "<gray>${spoiler}",
    "spoiler_symbol": "▌",
    "formatting": {
      "dark_red": true,
      "green": true,
      "underline": true,
      "dark_green": true,
      "black": true,
      "yellow": true,
      "bold": true,
      "italic": true,
      "dark_blue": true,
      "dark_purple": true,
      "gold": true,
      "red": true,
      "aqua": true,
      "gray": true,
      "light_purple": true,
      "blue": true,
      "white": true,
      "dark_aqua": true,
      "dark_gray": true,
      "spoiler": true,
      "strikethrough": true
    },
    "emoticons": {
      "item": "<click:suggest_command:':item:'><underline>[%player:equipment_slot mainhand%]",
      "shrug": "<click:suggest_command:':shrug:'>¯\\_(ツ)_/¯",
      "pos": "<click:suggest_command:':pos:'><bold>%player:pos_x% %player:pos_y% %player:pos_z%",
      "table": "<click:suggest_command:':table:'>(╯°□°）╯︵ ┻━┻",
      "playtime": "<click:suggest_command:':playtime:'><hover:'Total hours on this server'><dark_aqua><bold>%player:playtime%",
      "hp": "<click:suggest_command:':hp:'><hover:'Total number of half-hearts'><red><underline>%player:health% ❤"
    }
  },
  "styles": []
}